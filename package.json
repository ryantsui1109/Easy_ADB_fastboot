{
  "name": "easy_adb_fastboot",
  "version": "1.8.1",
  "description": "A GUI tool for easier using ADB and fastboot",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "clean": "modclean --ignore='platform-tools'",
    "clean-obf": "del *obfuscated.js",
    "obf": "node ./obfuscator.js"
  },
  "author": "ryantsui1109@github",
  "license": "Apache-2.0",
  "dependencies": {
    "@electron/asar": "^3.2.3",
    "@electron/osx-sign": "^1.0.4",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "electron-is-packaged": "^1.0.2",
    "got": "^11.8.5",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "electron": "^22.0.3",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "appId": "io.github.ryantsui1109.eaf",
    "productName": "Easy ADB and fastboot",
    "files": [
      "!{TODO.txt}"
    ],
    "extraFiles": "settings.json",
    "win": {
      "icon": "./favicon_256.ico",
      "extraFiles": "platform-tools-win",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "extraFiles": "platform-tools-linux",
      "target": [
        "tar.gz"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryantsui1109/Easy_ADB_fastboot.git"
  },
  "bugs": {
    "url": "https://github.com/ryantsui1109/Easy_ADB_fastboot/issues"
  },
  "homepage": "https://github.com/ryantsui1109/Easy_ADB_fastboot#readme"
}
